<AppChrome
  @editorDocument={{this.model.editorDocument}}
  @documentContainer={{this.model.documentContainer}}
  @exportHtmlFunction={{this.download}}
  @readOnly={{true}}
/>

<div class="editor-container container-flex--scroll">
  <div class="editor">
    <div class="au-o-grid au-o-grid--flush au-o-grid--reverse">
      <div class="au-o-grid__item au-u-1-3@medium">
        <div class="au-o-box">
        </div>
      </div>
      <div class="au-o-grid__item au-u-2-3@medium">
        <div class="editor__paper editor__paper--disabled editor__paper--full">
          {{!-- TODO: add wrapping vocab,prefix,type, etc --}}
          {{ this.model.editorDocument.htmlSafeContent }}
        </div>
      </div>

    </div>
  </div>
</div>

{{outlet}}
