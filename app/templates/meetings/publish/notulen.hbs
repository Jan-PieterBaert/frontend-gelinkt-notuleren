<div class="u-spacer--top">
  {{#if this.initializeNotulen.lastSuccessful}}
  <WuTimelineAccordion as |timeline|>
    <Signatures::Notulen::TimelineStep
            @name="notulen"
            @step={{timeline.step}}
            @notulen={{this.notulen}}
            @publicBehandelingUris={{publicBehandelingUris}}
            @behandelings={{behandelings}}
            @behandelingContainerId={{behandelingContainerId}}
            @zittingWrapper={{zittingWrapper}}
            @togglePublicationStatus={{togglePublicationStatus}}
            @signing={{perform this.createSignedResource}}
            @publish={{perform this.createPublishedResource}}

    />

  </WuTimelineAccordion>
  {{else}}
    <p>{{t "publish.loadingMessage"}}</p>
    <p class="loader"><span class="u-visually-hidden">{{t "publish.loadingMessage"}}</span></p>
  {{/if}}
</div>