<div class="u-spacer--top">
  {{#if this.initializeUittreksels.lastSuccessful}}
  <WuTimelineAccordion as |timeline|>
    {{#each this.uittreksels as |uittreksel|}}
      <Signatures::Behandeling::TimelineStep
        @name={{uittreksel.behandeling.onderwerp.titel}}
        @step={{timeline.step}}
        @behandeling={{uittreksel}}
        @signing={{perform this.createSignedResource uittreksel.behandeling}}
        @publish={{perform this.createPublishedResource uittreksel.behandeling}}
      />
    {{/each}}
  </WuTimelineAccordion>
  {{else}}
    <p>{{t "publish.loadingMessage"}}</p>
    <p class="loader"><span class="u-visually-hidden">{{t "publish.loadingMessage"}}</span></p>
  {{/if}}
</div>