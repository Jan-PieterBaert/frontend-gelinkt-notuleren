<AuCard @expandable="true" @flex="true" @divided="true" as |c|>
  <c.header>
    <AuHeading>
      Header
    </AuHeading>
  </c.header>
  <c.content>
    <div>
      Title: <Input type='text' @value={{mut this.title}} />
      Openbaar: <Input type='checkbox' @value={{mut this.openbaar}} />
    </div>
    <div>
      <BesluitDocumentContainer />
    </div>
  </c.content>
</AuCard>